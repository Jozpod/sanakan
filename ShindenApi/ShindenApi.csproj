<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netstandard2.0</TargetFrameworks>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <Version>0.3.8-beta</Version>
    <Authors>Jotpe</Authors>
    <Copyright>Copyright 2021 Â© Jotpe. All rights reserved.</Copyright>
    <PackageTags>Shinden REST API .NET shinden.pl wrapper</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>An official asynchronous .NET API Wrapper for the Shinden APIs.</Description>
    <PackageId>Shinden.NET</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />

  </ItemGroup>

</Project>
